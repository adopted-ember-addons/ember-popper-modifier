{
  "name": "ember-popper-modifier",
  "version": "2.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/alexlafroscia/ember-popper-modifier.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:md": "prettier --check '**/*.md'",
    "lint:json": "prettier --check '**/*.json'",
    "lint:yml": "prettier --check '**/*.yml'",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.2",
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-modifier": "^3.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.7.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.7.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "qunit-wait-for": "^2.0.0",
    "standard-version": "^9.0.0",
    "testdouble": "^3.16.0",
    "testdouble-qunit": "^3.0.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.hbs": "ember-template-lint",
    "*.js": "eslint --fix",
    "*.{css,json,md,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.4"
  }
}
