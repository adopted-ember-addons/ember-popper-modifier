{
  "name": "ember-popper-modifier",
  "version": "2.0.1",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/adopted-ember-addons/ember-popper-modifier.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint:md": "prettier --check '**/*.md'",
    "lint:json": "prettier --check '**/*.json'",
    "lint:yml": "prettier --check '**/*.yml'",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.2",
    "ember-auto-import": "^2.2.3",
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-modifier": "^3.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.9.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.3",
    "ember-resolver": "^8.0.0",
    "ember-source": "~4.10.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-ember": "^11.4.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^13.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.4",
    "prettier": "^2.0.5",
    "qunit": "^2.14.0",
    "qunit-dom": "^2.0.0",
    "qunit-wait-for": "^2.0.0",
    "standard-version": "^9.0.0",
    "testdouble": "^3.16.0",
    "testdouble-qunit": "^3.0.0",
    "webpack": "^5.58.2"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "lint-staged": {
    "*.hbs": "ember-template-lint",
    "*.js": "eslint --fix",
    "*.{css,json,md,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.4"
  }
}
